export CUDA_VISIBLE_DEVICES=1
python wgan.py --gen_iters 10000 --ncritic 30 --batch_size 500 --gen_lr 0.00005 --critic_lr 0.0001 --set_run 10 &&
python wgan.py --gen_iters 2500 --ncritic 30 --batch_size 1000 --gen_lr 0.000025 --critic_lr 0.00005 --load 10 10000 --set_run 11 &&
python wgan.py --gen_iters 1750 --ncritic 30 --batch_size 1000 --gen_lr 0.0000025 --critic_lr 0.000005 --load 11 2500 --set_run 12
